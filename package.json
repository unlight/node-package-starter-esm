  "description": "",
  "keywords": [],
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:r": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:w": "vitest watch",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "eslint src",
    "eslint:w": "onchange -i -k \"**/*.ts\" -- \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "format:src": "prettier src --write",
    "build": "sh Taskfile buildMicrobundle5",
    "lint:w": "run-p tscheck:w eslint:w",
    "test:m": "node node_modules/@stryker-mutator/core/bin/stryker.js run"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
