    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-typescript-paths": "^0.0.33",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "globals": "^16.5.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^3.7.4",
